@model Pseez.ViewModels.ViewModels.PseezEnt.Pmbok.ProjectViewModel

<div class="modal-header alert-info">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><span class="glyphicon glyphicon-remove"></span></button>
    <h3 class="modal-title">Project Details</h3>
</div>

<div class="modal-body">
    <div class="form-horizontal">
        <div class="form-group">
            @Html.LabelFor(m => Model.Name, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.EditorFor(m => m.Name, new { HtmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => Model.StartDate, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.EditorFor(m => m.StartDate, new { HtmlAttributes = new { @class = "form-control", type = "text", @disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => Model.EndDate, new { @class = "control-label col-sm-3" })
            <div class="col-sm-9">
                @Html.EditorFor(m => m.EndDate, new { HtmlAttributes = new { @class = "form-control" , type="text", @disabled = "disabled" } })
            </div>
        </div>
    </div>
</div>

<div class="modal-footer">
    <button class="btn btn-warning" data-dismiss="modal">Close</button>
</div>