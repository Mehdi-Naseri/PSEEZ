@model Pseez.Model.ViewModels.PseezEnt.Project.ProjectViewModel

    <div class="modal-header alert-info">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><span class="glyphicon glyphicon-remove closePanel"></span></button>
        <h3 class="modal-title">مشخصات پروژه</h3>
    </div>

    <div class="modal-body">
        <div class="form-horizontal">
            <div class="form-group">
                @Html.LabelFor(m => Model.Name, new { @class = "control-label col-sm-3" })
                <div class="col-sm-9">
                    @Html.TextBoxFor(m => m.Name, new { @class = "form-control", @disabled = "disabled" })
                </div>
            </div>
        </div>
    </div>

    <div class="modal-footer">
        <button class="btn btn-warning" data-dismiss="modal">بستن</button>
    </div>
